    "lint": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
  },
  "support": {
    "issues": "https://github.com/pdir/zero-one-wordpress-theme/issues",
    "source": "https://github.com/pdir/zero-one-wordpress-theme",
    "docs": "https://theme.kitchen/docs/",
    "forum": "https://theme.kitchen/forum/",
    "donate": "https://theme.kitchen/sponsoring.html"
